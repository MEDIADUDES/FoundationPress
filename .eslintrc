{
	"env": {
		"es6": true,
		"browser": true,
		"jquery": true
	},
	"extends": ["airbnb", "prettier", "prettier/react"],
	"plugins": ["prettier"],
	"rules": {
		"no-console": 1,
		"no-unused-vars": 1,
		"prettier/prettier": 2,
		"react/jsx-filename-extension": 0
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module",
		"allowImportExportEverywhere": false,
		"codeFrame": false
	}
}
